{
  "ios": {
    "trigger": "manual",
    "testEnabled": false,
    "signed": false,
    "environmentVariables": [{ "name": "CODE_PUSH_KEY", "value": "" }],
    "toolsets": {
      "xcode": {
        "xcodeVersion": "11.0",
        "automaticSigning": false,
        "projectOrWorkspacePath": "ios/NativeTemplate.xcworkspace",
        "podfilePath": "ios/Podfile",
        "scheme": "nativeTemplate"
      },
      "javascript": {
        "packageJsonPath": "package.json",
        "runTests": false,
        "nodeVersion": "10.x"
      },
      "buildscripts": {
        "package.json": {
          "postClone": "appcenter-post-clone.sh",
          "preBuild": "appcenter-pre-build.sh"
        }
      }
    }
  },
  "android": {
    "trigger": "manual",
    "testEnabled": false,
    "signed": false,
    "environmentVariables": [{ "name": "CODE_PUSH_KEY", "value": "" }],
    "toolsets": {
      "android": {
        "module": "app",
        "buildVariant": "debug",
        "isRoot": false,
        "runTests": false,
        "runLint": false,
        "automaticSigning": false,
        "gradleWrapperPath": "android/gradlew"
      },
      "javascript": {
        "packageJsonPath": "package.json",
        "runTests": false,
        "nodeVersion": "10.x"
      },
      "buildscripts": {
        "package.json": {
          "postClone": "appcenter-post-clone.sh",
          "preBuild": "appcenter-pre-build.sh"
        }
      }
    }
  }
}
